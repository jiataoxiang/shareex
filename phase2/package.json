{
  "name": "phase2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently \"nodemon server.js\" \"npm run client\"",
    "production-build": "cd client && npm install --only=dev && npm install && npm run build",
    "heroku-postbuild": "npm run production-build",
    "deploy": "npm run production-build && npm install && npm start"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cloudinary": "^1.17.0",
    "concurrently": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "date-and-time": "^0.11.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-form-data": "^2.0.10",
    "gridfs-stream": "^1.1.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lorem-ipsum": "^2.0.3",
    "mongodb": "^3.3.4",
    "mongoose": "^5.7.11",
    "morgan": "~1.9.1",
    "validator": "^12.0.0"
  }
}
