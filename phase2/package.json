{
  "name": "phase2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client": "cd client && npm start",
    "server": "nodemon ./bin/www",
    "dev": "concurrently \"nodemon ./bin/www\" \"npm run client\"",
    "production-build": "cd client && npm install --only=dev && npm install && npm run build",
    "heroku-postbuild": "npm run production-build",
    "deploy": "npm run production-build && npm install && npm start"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "concurrently": "^5.0.0",
    "config": "^3.2.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.4",
    "mongoose": "^5.7.11",
    "morgan": "~1.9.1",
    "validator": "^12.0.0"
  }
}
